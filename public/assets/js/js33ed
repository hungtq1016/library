		$( ".od-survey" ).click(function() {
		  $(this).addClass("od-survey-int");
		  $(".message-box").show();
		});
		// hover for desktop
		$( ".od-survey" ).hover(function() {
		  $(this).addClass("od-survey-int");
		  $(".message-box").show();
		});
	} else {
		$(".message-box, .od-survey").hide();
	}
	$( ".close-survey" ).click(function() {   
	  $(".message-box, .od-survey").hide();	  
	  $.cookie("od-survey", 1, { expires : 31, path: '/' });
    });

